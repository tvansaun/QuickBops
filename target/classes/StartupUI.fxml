<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text layoutX="125.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a folder containing mp3s" textAlignment="CENTER">
         <font>
            <Font name="Comic Sans MS" size="22.0" />
         </font>
      </Text>
      <TextField fx:id="textToMain" layoutX="125.0" layoutY="200.0" pickOnBounds="false" prefHeight="27.0" prefWidth="325.0" promptText="Enter file path or select from finder" />
      <Button fx:id="btnOpenMain" layoutX="261.0" layoutY="305.0" mnemonicParsing="false" text="Play!" />
      <Button fx:id="openFinder" layoutX="125.0" layoutY="227.0" mnemonicParsing="false" onAction="#openFinder" prefWidth="325.0" text="Open Finder..." />
      <Label fx:id="errorLabel" layoutX="215.0" layoutY="340.0" />
   </children>
</AnchorPane>
